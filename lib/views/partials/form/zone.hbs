<div id="ancgis-zoneform" class="ancgis-form" tabindex="0">
  <div class="ancgis-form-title">Propriétés de la zone de végétation</div>
  <div class="ancgis-form-fields">
    <div class="ancgis-form-field">
      <label for="ancgis-zoneform-typefield">Type de végétation : </label>
      <select id="ancgis-zoneform-typefield">
        <option value="" disabled selected>Sélectionner</option>
        <option value="forets">Forêts</option>
        <option value="prairies">Prairies</option>
        <option value="cultures">Cultures</option>
        <option value="landes">Landes</option>
        <option value="tourbieres">Tourbières</option>
        <option value="autre">Autre</option>
      </select>
    </div>
  </div>
  <div class="ancgis-form-ttbar">
    <span>Flore mellifère :</span>
    <button id="ancgis-zoneform-addspeciesbtn" class="btn btn-default" alt="Ajouter une espèce végétale" type="button" data-toggle="tooltip" data-placement="left" data-container="#ancgis-zoneform" title="Ajouter une espèce végétale."><span class="ancgis-glyphicons-enlarged ancgis-glyphicons-191plussign"></span></button>
  </div>
  <div class="ancgis-form-florefields">
    <table class="ancgis-form-floretable">
      <thead>
         <tr>
             <th>Espèce</th>
             <th class="ancgis-form-periodcolumn">Période(s)</th>
             <th class="ancgis-form-recoverycolumn">Rcvmt</th>
             <th class="ancgis-form-actioncolumn"></th>
         </tr>
      </thead>
      <tbody></tbody>
      </table>
  </div>
  <div class="ancgis-form-btbar">
    <button id="ancgis-zoneform-cancelbtn" class="btn btn-default" alt="Annuler les modifications" type="button" data-toggle="tooltip" data-placement="top" data-container="#ancgis-zoneform" title="Annuler les modifications."><span class="ancgis-glyphicons-enlarged ancgis-glyphicons-193removesign"></span></button>
    <button id="ancgis-zoneform-validatebtn" class="btn btn-default" alt="Valider les modifications" type="button" data-toggle="tooltip" data-placement="top" data-container="#ancgis-zoneform" title="Valider les modifications."><span class="ancgis-glyphicons-enlarged ancgis-glyphicons-194oksign"></span></button>
  </div>
</div>
