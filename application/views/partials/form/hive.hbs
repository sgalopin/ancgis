<div id="anc-hiveform" class="anc-form" tabindex="0">
  <div class="anc-form-title">Propriétés de la ruche</div>
  <div class="anc-form-fields">
    <div class="anc-form-field">
      <label for="anc-hiveform-registrationNumberfield">Numéro d'enregistrement : </label>
      <input type="text" id="anc-hiveform-registrationNumberfield" name="kok">
    </div>
    <div class="anc-form-field">
      <label for="anc-hiveform-typefield">Type de ruche : </label>
      <select id="anc-hiveform-typefield">
        <option value="" disabled selected>Sélectionner</option>
        <option value="dadant">Dadant</option>
        <option value="langstroth">Langstroth</option>
        <option value="warre">Warré</option>
        <option value="voirnot">Voirnot</option>
        <option value="kenyane">Kényane</option>
        <option value="tronc">Tronc</option>
        <option value="paille">Paille</option>
        <option value="bastian">Bastian</option>
        <option value="...">...</option>
      </select>
    </div>
    <div class="anc-form-field">
      <label for="anc-hiveform-framesCountfield">Nombre de cadre : </label>
      <select id="anc-hiveform-framesCountfield">
        <option value="" disabled selected>Sélectionner</option>
        <option value="5">5</option>
        <option value="6">6</option>
        <option value="10">10</option>
        <option value="12">12</option>
        <option value="...">...</option>
      </select>
    </div>
  </div>
  <div class="anc-form-btbar">
    <button id="anc-hiveform-cancelbtn" class="btn btn-default" alt="Annuler les modifications" type="button" data-toggle="tooltip" data-placement="top" title="Annuler les modifications."><span class="anc-glyphicons-enlarged anc-glyphicons-193removesign"></span></button>
    <button id="anc-hiveform-validatebtn" class="btn btn-default" alt="Valider les modifications" type="button" data-toggle="tooltip" data-placement="top" title="Valider les modifications."><span class="anc-glyphicons-enlarged anc-glyphicons-194oksign"></span></button>
  </div>
</div>
