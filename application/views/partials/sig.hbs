<!-- Map control tbar -->
<div id="anc-mapcontrol-tbar" class="btn-group">
  <button id="anc-mapcontrol-addhive" class="btn btn-default" alt="Ajouter une ruche" type="button" data-toggle="tooltip" data-placement="bottom" title="Ajouter une ruche. La commande &#171; ctrl + roulette &#187; permet la rotation de la ruche." data-shortid="addhive"><span class="anc-glyphicons anc-glyphicons-kiosk"></span></button>
  <button id="anc-mapcontrol-drawpolygon" class="btn btn-default" alt="Délimiter une zone de végétation" type="button" data-toggle="tooltip" data-placement="bottom" title="Délimiter une zone de végétation. Un &#171; clic droit &#187; sur une zone permet d'éditer ses propriétés." data-shortid="drawpolygon"><span class="anc-glyphicons anc-glyphicons-vectorpathpolygon"></span></button>
  <button id="anc-mapcontrol-drawcircle" class="btn btn-default" alt="Délimiter une zone de végétation" type="button" data-toggle="tooltip" data-placement="bottom" title="Délimiter un arbre solitaire. Un &#171; clic droit &#187; sur un arbre permet d'éditer ses propriétés." data-shortid="drawcircle"><span class="anc-glyphicons anc-glyphicons-vectorpathcircle"></span></button>
</div>
<!-- Map -->
<div id="anc-map" class="map"></div>
<!-- Map status tbar -->
<div id="anc-mapstatus-tbar">
  <div id="anc-mapstatus-projection">EPSG:3857</div>
  <div id="anc-mapstatus-mouseposition"></div>
</div>
