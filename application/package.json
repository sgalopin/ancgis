{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production ?? npm run start:prod || npm run start:dev",
    "start:prod": "NODE_ENV=production node ./bin/www",
    "start:dev": "NODE_ENV=development nodemon ./bin/www",
    "test": "mocha test/bootstrap.js --recursive test",
    "browserify": "mkdir -p public/javascript && browserify --debug -t [ hbsfy ] -g [ babelify --presets [ \"@babel/preset-env\" ] ] -g uglifyify --entry client/main.js > public/javascript/bundle.js"
  },
  "author": "Sylvain Galopin",
  "license": "GPLv3",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "~1.18.2",
    "bootstrap": "^3.3.7",
    "browserify-middleware": "^8.0.0",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "cssify": "^1.0.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.15.5",
    "express-flash": "0.0.2",
    "express-handlebars": "^3.0.0",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "handlebars": "^4.0.11",
    "hbsfy": "^2.8.1",
    "jquery": "^3.2.1",
    "mongoose": "^4.13.4",
    "morgan": "~1.9.0",
    "node-sass-middleware": "^0.11.0",
    "ol": "^5.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "serve-favicon": "~2.4.5",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babelify": "^9.0.0",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "connect-browser-sync": "^2.1.0",
    "lodash": "^4.17.10",
    "mocha": "^5.1.1",
    "nodemon": "^1.12.1",
    "puppeteer": "^1.3.0",
    "uglifyify": "^5.0.1"
  }
}
